datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model todo {
  id          String @id @default(cuid())
  title       String
  completed   Boolean
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
}